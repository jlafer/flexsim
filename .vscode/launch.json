{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Jest",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "GenConfig",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/genconfig.js",
      "args": [
        "-d",
        "domain",
        "-c",
        "config/flexstats"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "GenTiming",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/gentiming.js",
      "args": [
        "-c",
        "config/flexstats"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/deploy.js",
      "args": [
        "-c",
        "config/flexstats"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "FlexSim",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/flexsim.js",
      "args": [
        "-c",
        "config/flexstats",
        "-t",
        "60"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CustSim",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/custsim.js",
      "args": [
        "-c",
        "config/flexstats"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "AgentSim",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/agentsim.js",
      "args": [
        "-c",
        "config/flexstats"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CleanSim",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/cleansim.js",
      "args": [
        "-c",
        "config/flexstats",
        "--recordings",
        "2023-09-01"
      ]
    }
  ]
}