  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "GenConfig",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/genconfig.js",
      "args": [
        "-d",
        "domain",
        "-c",
        "config"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/deploy.js",
      "args": [
        "-c",
        "config"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CustSim",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/custsim.js",
      "args": [
        "-c",
        "config",
        "-t",
        "10"
      ]
    }
  ]
}